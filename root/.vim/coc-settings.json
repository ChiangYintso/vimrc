{
	"outline.keepWindow": false,
	"clangd.enabled": true,
	"clangd.arguments": [
		"--clang-tidy",
		"--compile-commands-dir=build/debug",
		"--pretty",
		"--cross-file-rename",
		"--inlay-hints=true",
		"--background-index",
		"--suggest-missing-includes=true"
	],
	"python.pythonPath": "/root/anaconda3/bin/python3",
	"python.linting.mypyEnabled": false,
	"python.linting.pylintEnabled": false,
	"pyright.enable": true,
	"pyright.testing.provider": "unittest",
	"rust-analyzer.enable": true,
	"workspace.ignoredFolders": [
		"$HOME",
		"$HOME/.cargo/**",
		"$HOME/.rustup/**"
	],
	"go.enable": true,
	"go.goplsPath": "/usr/bin/gopls",
	"java.enabled": true,
	"json.enable": true,
	"json.schemaDownload.enable": true,
	"sh.enable": true,
	"marksman.enable": true
}
