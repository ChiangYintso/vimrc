{
	"$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json#",
	"configurations": {
		"run": {
			"adapter": "delve",
			"default": true,
			"configuration": {
				"request": "launch",
				"program": "${file}",
				"cwd": "${workspaceRoot}",
				"args": [
					"*${CommandLineArgs}"
				],
				"console": "integratedTerminal",
				"stopOnEntry": true
			},
			"breakpoints": {
				"exception": {
					"raised": "N",
					"uncaught": "",
					"userUnhandled": ""
				}
			}
		}
	}
}
